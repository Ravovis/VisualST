<Window x:Class="VisualST.NewSequation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VisualST"
        mc:Ignorable="d"
        Title="NewSequation" Height="300" Width="400">
    <Grid>
        <StackPanel x:Name="RenamePanel" Margin="-10,0,10,0">
            <Label   Margin="130,20,10,0" Content="Выберите дейтвие"/>
            <ComboBox x:Name="ChoseAction" Margin="80,0,80,0">
            </ComboBox>
            <Button  x:Name="Create" Margin="100,80,100,10" Content="Создать" Height="40"/>

        </StackPanel>
    </Grid>
</Window>
