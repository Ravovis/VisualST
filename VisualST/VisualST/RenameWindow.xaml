<Window x:Class="VisualST.RenameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VisualST"
        mc:Ignorable="d"
        Title="RenameWindow" Height="166.596" Width="300">
    <Grid>
        <StackPanel x:Name="RenamePanel">
            <Label   Margin="10,10,10,10" Content="На что хотите переименовать?"/>
            <TextBox x:Name="RenameBox"  Margin="10,10,10,10" Height="23" TextWrapping="Wrap" />
            <Button  x:Name="RenameButton" Margin="10,10,10,10" Content="Button"/>

        </StackPanel> 
    </Grid>
</Window>
